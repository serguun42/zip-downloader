    "url": "git+https://github.com/serguun42/zip-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/serguun42/zip-downloader/issues"
  },
  "homepage": "https://github.com/serguun42/zip-downloader#readme",
  "dependencies": {
    "archiver": "5.3.1",
    "pm2": "5.2.0"
  },
  "devDependencies": {
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "typescript": "4.6.3"
